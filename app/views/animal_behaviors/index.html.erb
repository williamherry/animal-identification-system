<h1>Listing animal_behaviors</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Behavior</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @animal_behaviors.each do |animal_behavior| %>
      <tr>
        <td><%= animal_behavior.code %></td>
        <td><%= animal_behavior.behavior %></td>
        <td><%= link_to 'Show', animal_behavior %></td>
        <td><%= link_to 'Edit', edit_animal_behavior_path(animal_behavior) %></td>
        <td><%= link_to 'Destroy', animal_behavior, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Animal behavior', new_animal_behavior_path %>
